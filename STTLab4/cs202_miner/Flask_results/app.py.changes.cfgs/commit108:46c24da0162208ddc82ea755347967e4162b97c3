param start_response: a callable accepting a status code,\l                               a list of headers and an optional\l                               exception context to start the response\l        \"\"\"\l        ctx = self.request_context(environ)\l        ctx.push()\l        error = None\l        try:\l            try:\l                response = self.full_dispatch_request()\l            except Exception as e:\l                error = e\l                response = self.make_response(self.handle_exception(e))\l            return response(environ, start_response)\l        finally:\l            if self.should_ignore_error(error):\l                error = None\l            ctx.auto_pop(error)\l\l    @property\l    def modules(self):\l        from warnings import warn\l        warn(DeprecationWarning(\l            'Flask.modules is dep...'),\l            stacklevel=2)\l        return self.blueprints\l\l    def __call__(self, environ, start_response):\l        \"\"\"Shortcut for :attr:`...\"\"\"\l        return self.wsgi_app(environ, start_response)\l\l    def __repr__(self):\l        return '<%s %r>' % (self.__class__.__name__, self.name)\l" fillcolor="#FFFB81" shape=rectangle style="filled,solid"]
